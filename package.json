{
  "name": "typever",
  "version": "0.0.0-development",
  "description": "An opinionated versioning strategy for TypeScript ambient types",
  "main": "dist/commonjs/src/index.js",
  "module": "dist/modules/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "keywords": [
    "ember-addon",
    "typescript"
  ],
  "bin": "./bin/typever",
  "scripts": {
    "start": "node -r ts-node/register ./src/cli/index.ts",
    "problems": "node ./scripts/problems.js",
    "preversion": "npm run test",
    "prepare": "ember build -prod",
    "build": "ember build -prod",
    "build-tests": "ember build",
    "pretest": "npm run problems",
    "test": "ember test",
    "tdd": "ember test --server",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "devDependencies": {
    "@mike-north/js-lib-semantic-release-config": "0.0.0-development",
    "@types/chalk": "2.2.0",
    "@types/commander": "2.12.2",
    "@types/find-up": "2.1.1",
    "@types/inquirer": "0.0.43",
    "@types/node": "10.12.0",
    "@types/qunit": "2.5.3",
    "@types/semver": "5.5.0",
    "@types/window-size": "0.2.4",
    "ember-cli": "3.5.0",
    "libkit": "0.6.0",
    "qunit": "2.7.1",
    "semantic-release": "15.10.6",
    "shelljs": "0.8.2",
    "travis-deploy-once": "5.0.9",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^7.0.0",
    "exit": "^0.1.2",
    "find-up": "^4.0.0",
    "left_pad": "^0.0.11",
    "pkg-up": "^2.0.0",
    "resolve": "^1.8.1",
    "window-size": "^1.1.1",
    "word-wrap": "^1.2.3"
  },
  "toolchain": {
    "node": "10.9.0",
    "yarn": "1.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-north/typever.git"
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
